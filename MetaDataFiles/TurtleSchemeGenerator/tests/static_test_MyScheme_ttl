    sh:path rdf:type ;
  ] ;

  sh:property coscineSfb1394MyScheme:foo ;
  sh:property coscineSfb1394MyScheme:bar .

coscineSfb1394MyScheme:foo 
  sh:path sfb1394:foo ;
  sh:order 0 ;
  sh:datatype xsd:string ;
  sh:name "foo"@en, "foo"@de ;
  sh:path sfb1394:foo ;
. 

coscineSfb1394MyScheme:bar 
  sh:path sfb1394:bar ;
  sh:order 1 ;
  sh:datatype xsd:string ;
  sh:name "bar"@en, "bar"@de ;
  sh:path sfb1394:bar ;
. 
