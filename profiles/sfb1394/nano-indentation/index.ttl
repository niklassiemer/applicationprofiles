  sh:property coscineSfb1394NanoIndentaion:experimentId ;
  sh:property coscineSfb1394NanoIndentaion:operator ;
  sh:property coscineSfb1394NanoIndentaion:specimenId ;
  sh:property coscineSfb1394NanoIndentaion:parentSample ;
  sh:property coscineSfb1394NanoIndentaion:sampleLocation ;
  sh:property coscineSfb1394NanoIndentaion:Instrument ;
  sh:property coscineSfb1394NanoIndentaion:Tip ;
  sh:property coscineSfb1394NanoIndentaion:comments .

coscineSfb1394NanoIndentaion:experimentId
  sh:path dcterms:title;
  sh:order 0 ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
  sh:minLength 1 ;
  sh:datatype xsd:string ;
  sh:name "Experiment ID"@en, "Experiment ID"@de ;
.

