coscineSfb1394NanoIndentaion:operator
  sh:path dcterms:creator ;
  sh:order 1 ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
  sh:minLength 1 ;
  sh:datatype xsd:string ;
  sh:defaultValue "{ME}" ;
  sh:name "Operator"@en, "Operator"@de ;
.

coscineSfb1394NanoIndentaion:specimenId
  sh:path csmd:investigation_sample ;
  sh:order 3 ;
  sh:maxCount 1 ;
  sh:datatype xsd:string ;
  sh:name "Specimen ID"@en, "Specimen ID"@de ;
.

coscineSfb1394NanoIndentaion:parentSample
  sh:path sfb1394:parentSample ;
  sh:order 4 ;
  sh:maxCount 1 ;
  sh:datatype xsd:string ;
  sh:name "Parent Sample"@en, "Parent Sample"@de ;
.

coscineSfb1394NanoIndentaion:sampleLocation
  sh:path sfb1394:sampleLocation ;
  sh:order 4 ;
  sh:maxCount 1 ;
  sh:datatype xsd:string ;
  sh:name "Sample location"@en, "Sample location"@de ;
.

coscineSfb1394NanoIndentaion:Instrument
  sh:path sfb1394:Instrument ;
  sh:order 4 ;
  sh:maxCount 1 ;
  sh:datatype xsd:string ;
  sh:name "Instrument used"@en, "Instrument used"@de ;
.

coscineSfb1394NanoIndentaion:Tip
  sh:path sfb1394:Tip ;
  sh:order 4 ;
  sh:maxCount 1 ;
  sh:datatype xsd:string ;
  sh:name "Tip used"@en, "Tip used"@de ;
.

coscineSfb1394NanoIndentaion:comments
  sh:path dcterms:description ;
  sh:order 16 ;
  sh:maxCount 1 ;
  sh:datatype xsd:string ;
  dash:singleLine false ;
  sh:name "Comments"@en, "Comments"@de ;
.
